<set>
    <broadcast>
        <action>com.atakmap.android.features.SHOW_EDIT</action>
        <extras>
            <long key='fid'>${fid}</long>
            <string-array key='fsids'>${fsids}</string-array>
            <string key='title'>${title}</string>
        </extras>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.UNFOCUS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_DETAILS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_MENU</action>
    </broadcast>
</set>
